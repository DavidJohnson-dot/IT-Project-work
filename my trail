      "source": [
        " import pandas as pd\n",
        " import numpy as np\n",
        "\n",
        " #Customers number\n",
        " n_customers = 200\n",
        "\n",
        " #table\n",
        " data = {\n",
        "     'CustomerID':[f'C{str(i).zfill(3)}' for i in range (1, n_customers + 1)],\n",
        "     'Age': np.random.randint(18,40, size=n_customers),\n",
        "     'Gender': np.random.choice(['Male', 'Female', 'other'], size=n_customers),\n",
        "     'Location': np.random.choice(['Ashaiman', 'Accra Mokola', 'Teshie', 'Labadi', 'Osu', 'Tema'], size=n_customers),\n",
        "      'ProductViewed': np.random.randint(1,20, size=n_customers),\n",
        "     'TimeSpent(minute)': np.random.randint(1, 24, size=n_customers),\n",
        "     'NumberOfPurchases': np.random.randint(0, 10, size=n_customers),\n",
        "     'MostVisitedCategory': np.random.choice(['Fashion','Electronics', 'Beauty and Skincare', 'Home Decor', 'Kitchenware'], size=n_customers),\n",
        "      'TotalAmountPaid GH₵': np.random.uniform(10.0, 1000.0, size=n_customers),\n",
        "     'LastPurchasedate': pd.to_datetime('2025-01-01') + pd.to_timedelta(np.random.randint(0, 100, size=n_customers), unit='d')\n",
        " }\n",
        " df=pd.DataFrame(data)"
      ]
    },
